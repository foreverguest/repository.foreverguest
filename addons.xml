<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addons>
<addon id="plugin.audio.googlemusic.exp"
       name="Google Music EXP"
       version="1.11.1"
       provider-name="ForeverGuest">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
    <import addon="script.module.decorator" version="3.4.0"/>
    <import addon="script.module.requests" version="1.1.0"/>
  </requires>
  <extension point="xbmc.python.pluginsource" library="default.py">
    <provides>audio</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
    <platform>all</platform>
    <summary lang="en">Experimental Google Music audio plugin</summary>
    <description lang="en">EXPerimental plugin that lets you play music from Google Music.</description>
  </extension>
</addon>

<addon id="repository.foreverguest" name="foreverguest Add-ons" version="1.1.0" provider-name="foreverguest">
    <extension point="xbmc.addon.repository" name="foreverguest Add-ons">
        <info compressed="true">https://github.com/foreverguest/repository.foreverguest/raw/master/addons.xml</info>
        <checksum>https://github.com/foreverguest/repository.foreverguest/raw/master/addons.xml.md5</checksum>
        <datadir zip="true">https://github.com/foreverguest/repository.foreverguest/raw/master/</datadir>
    </extension>
    <extension point="xbmc.addon.metadata">
        <summary>foreverguest Add-ons</summary>
        <platform>all</platform>
    </extension>
</addon>

<addon id="script.module.decorator"
       name="decorator"
       version="3.4.0"
       provider-name="Valentin-Costel HÄƒloiu">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
  </requires>
  <extension point="xbmc.python.module"
             library="lib" />
  <extension point="xbmc.addon.metadata">
    <summary>Python decorator library</summary>
    <description>Packed for XBMC from http://code.google.com/p/micheles/source/browse/decorator</description>
    <license>BSD</license>
    <platform>all</platform>
	<source>http://code.google.com/p/micheles/source/browse/decorator</source>
  </extension>
</addon>
</addons>
